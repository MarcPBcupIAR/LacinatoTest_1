<?xml version="1.0" encoding="iso-8859-1"?>

<webabx_fileset>

  <!-- each menu needs a unique ID -->
  <!-- each menu should have a unique "pretty name" for display -->
  <menu id="audiosource" default="Guitar">Audio Source</menu>
  <menu id="mic">Mic</menu>
  <menu id="distance" default="2 feet">Distance</menu>

  <!-- <menuimages> id must exactly match the "menu id" defined in the <menu> tag. -->
  <!-- The keys here should be exactly as defined in the <file> definition below. -->
  <!-- You don't have to have a <menuimages> for all menus, just the ones you care about. -->
  <!-- For a given <menuimages>, you don't have to define a value for every possible key
       that might appear in that menu, only the ones you want to see images for. -->
  <menuimages id="audiosource">
    <menuimage key="Mandolin" value="demo_preview_images/mandolin.jpg" />
    <menuimage key="Guitar" value="demo_preview_images/guitar.jpg">This is the caption for the Guitar image.</menuimage>
    <menuimage key="Piano" value="demo_preview_images/piano.jpg">This is the caption for the Piano image.</menuimage>
  </menuimages>

  <menuimages id="mic">
    <menuimage key="AKG C535" value="demo_preview_images/535.jpg">AKG C535-EB small diaphragm condenser</menuimage>
    <menuimage key="CAD VX2" value="demo_preview_images/vx2.jpg">CAD VX2 US-made tube mic with OS125 capsule</menuimage>
    <menuimage key="Homemade Electret" value="demo_preview_images/electret.jpg">A cheap/simple home-made mic with a Primo EM184 capsule in it.</menuimage>
  </menuimages>
  
  <!-- files can use the menu IDs from above. E.g. if you have
       <menu id="geartype">Gear Type</menu>
       
        ...then in the file section you can use:

        <file path="/audio/blah.mp3"
              geartype="Equalizers"
         />

         That will create a menu titled "Gear Type" with an entry
         in it called "Equalizers" which, when used to filter, will
         include this file.
         
         Make sure that the menu id "geartype" matches exactly the
         file attribute "geartype=". Similarly the values of these
         parameters (meaning 'compressor' in geartype="compressor")
         should match each other exactly.
  -->

